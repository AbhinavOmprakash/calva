            },
            {
                "command": "calva.evaluateFile",
                "key": "ctrl+alt+v enter"
            },
            {
                "command": "calva.lintFile",
                "key": "ctrl+alt+v l"
            },
            {
                "command": "calva.runNamespaceTests",
