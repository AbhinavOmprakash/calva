{
	"name": "clojure-warrior",
	"displayName": "Clojure Warrior",
	"description": "Extension for Clojure development",
	"version": "0.1.0",
	"publisher": "tonsky",
	"license": "MIT",
	"maintainers": [
		"Nikita Prokopov <prokopov@gmail.com>"
	],
	"engines": {
		"vscode": "^1.15.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
    "bracket",
    "brackets",
    "match",
    "matching",
		"clojure"
  ],
	"activationEvents": [
		"onLanguage:clojure"
	],
	"icon": "extras/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/tonsky/clojure-warrior.git"
	},
	"bugs": {
		"url": "https://github.com/tonsky/clojure-warrior/issues"
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"vscode": "^1.1.5",
		"typescript": "^2.5.3",
		"@types/node": "7.0.4"
	}
}
